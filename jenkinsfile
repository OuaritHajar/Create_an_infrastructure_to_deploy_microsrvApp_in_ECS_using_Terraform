pipeline {
    agent any
    environment {
        AWS_DEFAULT_REGION = 'us-east-1' 
    }
    stages {
        stage('Clone repository') {
            steps {
                 script{
                     git(
                         url: "",
                         branch: "main",
                         changelog: true,
                         poll: true
                        )
                 }
            } 
        }
        stage('Pull the images') {
            steps {
                sh 'docker pull api'
                sh 'docker pull client'
                sh 'docker pull mongo'
        }
